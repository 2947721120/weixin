<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yihou.dao.AsmInDao">
    <!-- 根据用户编号查询用户 -->
    <select id="findAsmInHead" resultType="hashmap">
        select a.*,b.ParComNameC
        from AsmInHead a left join Custom b on a.Customcode = b.ParComID
        <where>
            <if test="params.beginDate != null">
                a.AsmDT <![CDATA[>=]]> #{beginDate}
            </if>
            <if test="params.endDate != null">
                AND a.AsmDT <![CDATA[<=]]> #{endDate}
            </if>
        </where>
    </select>
</mapper>